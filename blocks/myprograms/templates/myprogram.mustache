
<div class="accordion" id="accordionExample">
{{#program}}
<div style="padding:30px 0;">
  <span style="font-weight:700;font-size:20px;">{{name}}</span><span style="float:right;font-weight:700;font-size:15px;">in-progress</span>
</div>
{{/program}}
{{#levelloop}}
  <div class="card">
    <div class="card-header" id="headingOne">
      <h2 class="mb-0">
        <button class="btn btn-link btn-block text-left collapsed" type="button" data-toggle="collapse" data-target="#collapse{{id}}" aria-expanded="true" aria-controls="collapseOne">
          {{level}} <i class="fa fa-arrow-down" aria-hidden="true" style="float:right"></i>

        </button>
      </h2>
    </div>

    <div id="collapse{{id}}" class="collapse" aria-labelledby="headingOne" data-parent="#accordionExample">
      <div class="card-body">
        {{description}}
      </div>
    </div>
  </div>
  {{/levelloop}}
  