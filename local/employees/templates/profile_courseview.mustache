<div>
   <table class="generaltable" id="popup_courseview">
            <thead>
                <th>{{# str}}courses, local_employees{{/ str}}</th>
                <th>{{# str}}student, local_users{{/ str}}</th>
                <th>{{# str}}attendance, local_employees{{/ str}}</th>

            </thead>
        <tbody>
            {{#records}}
            <tr>
                <td> <a href="{{url}}">{{ fullname }}</a></td>
                <td><button class="rolesstudentpopup" href="javascript:void(0)" data-id={{cid}} data-teacherid={{teacherid}} data-open_identifiedas={{open_identifiedas}} data-action="popupmodal{{id}}">{{ studentcount }}</button></td>
                <td>{{percentage}}%</td>      

           </tr>
            {{/records}}
        </tbody>
    </table>
</div>
